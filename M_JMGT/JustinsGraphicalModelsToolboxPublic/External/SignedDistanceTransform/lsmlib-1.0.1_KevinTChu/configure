  enableval=$enable_profile; enable_profile=yes
else
  enable_profile=no
fi


# Check whether --enable-mode was given.
if test "${enable_mode+set}" = set; then
  enableval=$enable_mode; case $enableval in
                 opt|debug|profile) build_mode=$enableval ;;
                 *) { { echo "$as_me:$LINENO: error:     unrecognized mode --enable-mode=$enableval; use \`opt',
    \`debug', or \`profile'" >&5
echo "$as_me: error:     unrecognized mode --enable-mode=$enableval; use \`opt',
    \`debug', or \`profile'" >&2;}
   { (exit 1); exit 1; }; } ;;
               esac
fi


# build mode priorities:  profile, debug, optimize
# default build mode:     optimize
if test $enable_profile = yes; then
  build_mode=profile
else
  if test $enable_debug = yes; then
  build_mode=debug
else
  if test $enable_opt = yes; then
  build_mode=optimize
else
  build_mode=optimize
fi


fi


fi


