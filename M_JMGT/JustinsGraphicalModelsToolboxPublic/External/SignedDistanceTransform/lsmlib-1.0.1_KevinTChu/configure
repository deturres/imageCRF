# set compiler flags to build position-independent code
case "$CC" in
  gcc)
    CFLAGS="-fPIC $CFLAGS"
  ;;
  xlc)
    CFLAGS="-fPIC $CFLAGS"
  ;;
  icc)
    CFLAGS="-fPIC $CFLAGS"
  ;;
  blrts_xlc)
    # no position-independent code for BlueGene/L
  ;;
  *)
    # do not use compiler flags for position-independent code
esac

case "$CXX" in
  g++)
    CXXFLAGS="-fPIC $CXXFLAGS"
  ;;
  xlC)
    CXXFLAGS="-fPIC $CXXFLAGS"
  ;;
  icpc)
    CXXFLAGS="-fPIC $CXXFLAGS"
  ;;
  blrts_xlC|blrts_xlc++)
    # no position-independent code for BlueGene/L
  ;;
  *)
    # do not use compiler flags for position-independent code
esac

case "$F77" in
